{
  "configurations": [
    {
      "name": "Linux",
      "includePath": [
        "${workspaceFolder}/**",
        "${workspaceFolder}/include",
        "${workspaceFolder}/src/include",
        "${workspaceFolder}/duckdb/src/include",
        "${workspaceFolder}/duckdb/third_party/re2",
        "${workspaceFolder}/build/release/duckdb/src/include",
        "${workspaceFolder}/vcpkg_installed/x64-linux/include"
      ],
      "defines": ["_DEBUG", "UNICODE", "_UNICODE"],
      "compilerPath": "/usr/bin/gcc",
      "cStandard": "c11",
      "cppStandard": "c++11",
      "intelliSenseMode": "linux-gcc-x64",
      "configurationProvider": "ms-vscode.cmake-tools",
      "compileCommands": "${workspaceFolder}/build/clangd/compile_commands.json"
    }
  ],
  "version": 4
}
